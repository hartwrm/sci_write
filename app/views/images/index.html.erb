<p id="notice"><%= notice %></p>

<div class="title">
  <h1>Images</h1>
</div>

<div class="image-container">
 <% @images.each do |image| %>
   <%= link_to image_path(image) do %>
   <%= image_tag image.imagedata.thumb.url, class: "image-links" %>
   <% end %>
 <% end %>

</div>

<br>
<div class="foot">
  <%= link_to 'New Image', new_image_path, class: "text-link" %>
  <br>
  <%= link_to "Home", welcome_index_url, class: "text-link" %>
</div>
